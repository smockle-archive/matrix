{
  "name": "@smockle/matrix",
  "version": "1.0.2",
  "description": "Single and multi dimensional matrices and matrix functions.",
  "main": "index.js",
  "scripts": {
    "docs": "jsdoc2md \"lib/**/*.js\" --template jsdoc2md/README.hbs --example-lang JavaScript > README.md",
    "test": "npm run test:bash",
    "test:bash": "tape \"test/**/*.js\" && snyk auth $SNYK_TOKEN && snyk test",
    "test:cmd": "tape \"test/**/*.js\" && snyk auth %SNYK_TOKEN% && snyk test",
    "coverage": "istanbul cover tape \"test/**/*.js\"",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "postpublish": "greenkeeper-postpublish",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smockle/matrix.git"
  },
  "author": "Clay Miller <clay@smockle.com> (https://smockle.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smockle/matrix/issues"
  },
  "homepage": "https://github.com/smockle/matrix#readme",
  "devDependencies": {
    "codeclimate-test-reporter": "^0.4.0",
    "coveralls": "^2.11.9",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^10.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "greenkeeper-postpublish": "^1.0.0",
    "husky": "^0.13.2",
    "istanbul": "^0.4.3",
    "jsdoc-to-markdown": "^3.0.0",
    "lint-staged": "^3.4.0",
    "prettier-eslint-cli": "^3.1.2",
    "tape": "^4.5.1"
  },
  "snyk": true,
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/prettier-eslint --write",
      "git add"
    ]
  },
  "dependencies": {
    "lodash": "^4.11.2",
    "mathjs": "^3.2.1",
    "snyk": "^1.21.1"
  }
}
